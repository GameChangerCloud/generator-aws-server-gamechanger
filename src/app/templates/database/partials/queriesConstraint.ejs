const queriesConstraint = [
<%_ tables.forEach(table => { for (let index = 0; index < table.columns.length; index++) { _%>
    <% if (table.columns[index].constraint != null && table.columns[index].constraint.includes("FOREIGN KEY")) { %>
        {tableName: "<%= table.sqlName %>", text: `ALTER TABLE "<%= table.sqlName %>" ADD <%- table.columns[index].constraint %> ON DELETE SET NULL DEFERRABLE INITIALLY DEFERRED`},
<% } %>
<% }}); %>
    ]