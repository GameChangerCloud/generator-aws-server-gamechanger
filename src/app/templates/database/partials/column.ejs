"<%= column.field %>" <% if (column.field.startsWith("Pk")) { -%> <%=column.constraint-%><% } else { -%>
    <%- column.fieldType -%>
    <%_ if (column.isArray) { -%> ARRAY<% } -%>
    <%_ if (column.noNull) { -%> NOT NULL<% } -%>
    <%_ if (column.unique) { -%> UNIQUE<% } -%>
    <%_ if (column.noNullArrayValues) { -%> constraint not_null_inside_array check( array_position(<%=column.field %>, null) is null)<% } -%>
<% } -%>