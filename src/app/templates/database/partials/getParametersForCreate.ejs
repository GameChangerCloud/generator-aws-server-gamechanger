<% for (let index = 0; index < table.length; index++) { _%>
    <%if (table[index].isArray) { _%>
       <% switch (table[index].gqlType) { 
            case "String": _%>
                constructChanceListString(<%- tableName %>)
                <%break;
            case "ID": _%>
                generateRandomListID()
                <%break;
            case "Boolean": _%>
                generateRandomListBoolean()
                <%break;
            case "Int": _%>
                generateRandomListInt()
                <%break;
            default: _%>
                null
                <%break;
       }
    }
    else {%>
        <%switch (table[index].gqlType) { 

            case "String": _%>
                <%- matching.matchString( tableName ) _%>
                <% break;
            case "ID": _%>
                pad(chance.integer({ min: -2147483647, max: 2147483647 }), 3)
                <%break;
            case "Boolean": _%>
                chance.bool()
                <%break;
            case "Int": _%>
                chance.integer({ min: -2147483647, max: 2147483647 })
                <%break;
            case "Date": _%>
                getRandomDate()
                <% break
            case "DateTime": _%>
                getRandomDateTime().toISOString()
                <%break
            case "Time": _%>
                getRandomTime()
                <% break
            case "RGBA": _%>
                `rgba(${chance.color({format: 'rgb'})}, ${chance.color({format: 'rgb'})}, ${chance.color({format: 'rgb'})}, ${Math.random()})`
                <% break
            case "RGB": _%>
                chance.integer({ min: -2147483647, max: 2147483647 })
                <%break;
            case "URL":_%>
                <%if (tableName.includes("avatar") || tableName.includes("profile")){%>
                    chance.avatar()
                <%}else if (tableName.includes("image") || tableName.includes("picture")){%>
                    chance.avatar() 
                <%}else{%>
                    chance.url()
                <%}%>
                <%break
            case "EmailAddress": _%>
                chance.email()
                <%break
            case "HexColorCode":_%>
                chance.color()
                <%break
            // TODO : Handle all the scalar type from graphql-scalar module, see chance
            default:_%>
                null
                <%break;
        }%>
    <%}%>
    , 
<%}%>


