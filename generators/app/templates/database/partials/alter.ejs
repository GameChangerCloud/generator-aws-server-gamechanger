<% switch (alterCase) {
    case "AddColumn" : _%>
        {tableName: "<%= name %>", text: `ALTER TABLE "<%= name %>" ADD COLUMN <%- columns.field %> <%- columns.fieldType %>`},
            <% break;
    case "Deferred" : _%>
        <% if (columns.constraint != null && columns.constraint.includes("FOREIGN KEY")) { %>
        {tableName: "<%= name %>", text: `ALTER TABLE "<%= name %>" ADD <%- columns.constraint %> DEFERRABLE INITIALLY DEFERRED`},
        <% }break;
    case "AddColumnNotNull" :
            let d = new Date(); _%>
        {tableName: "<%= name %>", text: `ALTER TABLE "<%= name %>" ADD COLUMN <%- columns.field %> <%- columns.fieldType %> NOT NULL DEFAULT <%- include('./initType.ejs',{type : columns.fieldType}) %>`},
        <% break;
    case "AlterColumn" : _%>
        {tableName: "<%= name %>", text: `ALTER TABLE "<%= name %>" ALTER COLUMN <%- field %> DROP <%= drop %>`},
        <% break;
    case "AlterType" : _%>
        {tableName: "<%= name %>", text: `ALTER TABLE "<%= name %>" ALTER COLUMN "<%= old_name %>" TYPE <%= type %> USING "<%= old_name %>"<%= defaultInit %>;`},
        <% break;
    case "Update" : _%>
        {tableName: "<%= name %>", text: `UPDATE "<%= name %>" SET "<%= old_name %>" = <%- include('./defaultVal',{type: default_val}) _%> <%- set %>;`},
        <% break;
    case "AlterColumnSet" : _%>
        {tableName: "<%= name %>", text: `ALTER TABLE "<%= name %>" ALTER COLUMN "<%= old_name %>" SET <%= datatype %> ;`},
        <% break;
    case "AlterUsingCase" : _%>
        {tableName: "<%= name %>", text: `ALTER TABLE "<%= name %>" ALTER "<%= old_name %>" TYPE <%= datatype %> USING CASE WHEN "<%= old_name %>"=<%= defaultInit %>;`},
        <% break; 
    default : _%>
            <% break;
} _%>