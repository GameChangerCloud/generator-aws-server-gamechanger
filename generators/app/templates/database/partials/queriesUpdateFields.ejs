        <%}
        else { console.log("Not supported") }
    }
    if (!x.column_old.noNull && x.column_new.noNull) { _%>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, default_val: x.column_new.type, set :' WHERE "' + x.column_old.name + '" IS NULL', alterCase: "Update"}) %>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, datatype: "NOT NULL", alterCase : "AlterColumnSet"}) %>
    <%}
    else if (x.column_old.noNull && !x.column_new.noNull) {_%>
<%- include('./alter',{name: x.name, field: x.column_old.name, drop: "NOT NULL", alterCase : "AlterColumn"}) %>
    <% }}) _%>
    ]