<%- include('./alter',{name: x.name, old_name: x.column_old.name, type: "INT",defaultInit: "::integer", alterCase : "AlterType"}) %>
        <% }
        else if (x.column_old.type == "Int" && x.column_new.type == "String") { _%>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, datatype: "DATA TYPE text", alterCase : "AlterColumnSet"}) %>
        <% }
        else if (x.column_old.type == "String" && x.column_new.type == "Boolean") { _%>
<%- include('./alter',{name: x.name, field: x.column_old.name, drop: "DEFAULT", alterCase : "AlterColumn"}) %>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, type: "bool", defaultInit: "\'true\' THEN TRUE ELSE FALSE END", alterCase : "AlterUsingCase"}) %>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, datatype: "DEFAULT FALSE", alterCase : "AlterColumnSet"}) %>
        <% }
        else if (x.column_old.type == "Boolean" && x.column_new.type == "String") { _%>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, datatype: "DATA TYPE text", alterCase : "AlterColumnSet"}) %>
        <%}
        else if (x.column_old.type == "Boolean" && x.column_new.type == "Int") { _%>
<%- include('./alter',{name: x.name, field: x.column_old.name, drop: "DEFAULT", alterCase : "AlterColumn"}) %>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, type: "INT", default: "FALSE THEN 0 ELSE 1 END", alterCase : "AlterUsingCase"}) %>
<%- include('./alter',{name: x.name, old_name: x.column_old.name, datatype: "DEFAULT 0", alterCase : "AlterColumnSet"}) %>
        <%}
        else if (x.column_old.type == "Int" && x.column_new.type == "Boolean") { _%>
<%- include('./alter',{name: x.name, field: x.column_old.name, drop: "DEFAULT", alterCase : "AlterColumn"}) %>
